<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zker.model.main.SysMain" table="SYS_MAIN">
        <id name="mainId" type="int" column="MAIN_ID">
            <generator class="sequence">
                <param name="sequence">SEQ_SYS_MAIN</param>
            </generator>
        </id>
        <property name="imageName" type="string" column="IMAGE_NAME" />
        <property name="imageUrl" type="string" column="IMAGE_URL" />
    </class>

</hibernate-mapping>